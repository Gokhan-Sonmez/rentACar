package com.btkAkademi.rentACar.entities.concretes;

import java.util.List;

import javax.persistence.OneToMany;

import lombok.AllArgsConstructor;
import lombok.Data;
import lombok.NoArgsConstructor;

@Data
@AllArgsConstructor
@NoArgsConstructor
public class Brand {

	private int id;
	private String name;
	
	@OneToMany
	private List<Car> cars;
	
}
