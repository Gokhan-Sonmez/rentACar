import javax.validation.constraints.Size;

import com.btkAkademi.rentACar.business.constants.Messages;

public class CreateBrandRequest {

	@NotBlank
	@Size(min=3, max=20, message = Messages.INVALIDBRANDNAME)
	private String name;
}
